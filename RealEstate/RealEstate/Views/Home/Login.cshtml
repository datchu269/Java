@model RealEstate.Models.Account

@{
    ViewData["Title"] = "Create";
}


<!-- Main -->


<section class="login-regis" style="padding-top: 105px;">
    <div class="container-login">
        <h2>Đăng nhập</h2>
        <form asp-action="Login">
            <div class="form-login-group">
                <label for="email">Email</label>
                <input type="email" id="email" placeholder="Nhập email của bạn">
            </div>
            <div class="form-login-group">
                <label for="pass-login">Mật khẩu</label>
                <input type="password" id="pass-login" placeholder="Nhập mật khẩu của bạn">
            </div>
            <div class="form-login-group re-pass">
                <input type="checkbox" id="remember-pass">
                <label for="remember-pass">Nhớ mật khẩu</label>
            </div>
            <div class="form-login-submit">
                <button>Xác nhận</button>
            </div>
        </form>
    </div>
</section>
